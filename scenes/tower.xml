        <material id="metal" shininess="15">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0" g="0.588" b="1.0" a="1.0" />
            <diffuse r="0" g="0.588" b="1.0" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>
        <material id="stone" shininess="5">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />

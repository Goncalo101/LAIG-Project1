<lxs>
    <scene root="demoRoot" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
        <spot id="cannonLight" enabled="0" angle="10" exponent="10">
            <location x="20" y="10" z="25" w="1" />
            <target x="20" y="0" z="25" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.625" g="0.32" b="0.175" a="1.0" />
            <specular r="0.625" g="0.32" b="0.175" a="0.5" />
            <attenuation constant="0" linear="1" quadratic="0" />
        </spot>
    </lights>

    <textures>
        <!-- Skybox textures-->
        <texture id="sb_down" file="scenes/images/hills_dn.jpg" />
        <texture id="sb_back" file="scenes/images/hills_bk.jpg" />
        <texture id="sb_front" file="scenes/images/hills_ft.jpg" />
